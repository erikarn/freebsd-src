#
# Qualcomm SoC support
#

# Qualcomm Snapdragon drivers
device		qcom_gcc		# Global Clock Controller
device		qcom_mdio		# MDIO support

# Serial (COM) ports
device		uart_msm		# Qualcomm MSM UART driver

# TLMM driver (commented out for bring-up module)
# device		qcom_tlmm

# MMC/SD/SDIO Card slot support
device		sdhci

options 	FDT

# DTBs
makeoptions	MODULES_EXTRA+="dtb/qcom"

# options 	FDT_DTB_STATIC
# makeoptions	FDT_DTS_FILE=qcom/x1e78100-lenovo-thinkpad-t14s.dts
