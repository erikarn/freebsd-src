// Load at offset 0x81000000 (16mb)
// FDT for now is 0x83000000 (48mb) - I'm not sure yet if it's used or needed
// Empty ramdisk for now

/ {
        description = "FreeBSD-HEAD AC1300 FIT Image";
        #address-cells = <1>;

        images {
                kernel {
                        description = "Kernel";
                        data = /incbin/("../../kernel.elf.lzma");
                        type = "kernel";
                        arch = "arm";
                        os = "linux";
                        compression = "lzma";
                        load = <0x81000000>;
                        entry = <0x81000200>;
                        hash {
                                algo = "sha1";
                        };
                };
